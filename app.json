{
  "expo": {
    "name": "GlowgetterApp",
    "slug": "glowgetter-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "glowgetter",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.penelope11.glowgetterapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.penelope11.glowgetterapp",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS",
        "WAKE_LOCK"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon/notification-icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/firestore"
    ],
    "extra": {
      "eas": {
        "projectId": "eeb29cff-888f-4029-9650-5ebf124cbcd2"
      }
    },
    "owner": "penelope11"
  }
}
